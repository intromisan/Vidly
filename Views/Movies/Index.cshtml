@model IEnumerable<Vidly.Models.Movie>

@{
    ViewData["Title"] = "Movies";
}

<h2>Movies</h2>

<table class="table table-bordered table-hover">
    <thead>
        <tr>
            Movie
        </tr>
    </thead>
    <tbody>
        @foreach ( var movie in Model)
        {
            <tr>
                <td>@movie.Title</td>
            </tr>
        }
    </tbody>
</table>
